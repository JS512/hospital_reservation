<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sbs.cuni.dao.CounselReservationDao">

	<select id="getOneReservation" parameterType="long" resultMap="CounselReservation">
		SELECT *
		FROM CounselReservation
		WHERE clientMemberId = #{loginedMemberId}
	</select>

	<resultMap type="com.sbs.cuni.dto.CounselReservation" id="CounselReservation">
		<id property="id" column="id" />
		<id property="regDate" column="regDate" />
		<id property="clientMemberId" column="clientMemberId" />
		<id property="staffScheduleId" column="staffScheduleId" />
		<id property="body" column="body" />
	</resultMap>
</mapper>